<div class="row">
        <div class="col-md-6 col-sm-6 col-xs-12">
        <div class="x_panel fixed_height_500">

          <div class="x_content">
            <h4>UA Nighlty Tests</h4>

            <% if ! @ua_nightly.nil? &&  @ua_nightly.any?%>

              <% @ua_nightly.each do |env,env_run| %>
                <h2><%= env %></h2>
                  <% env_run.sort_by { |n| -n[:execution_status] }.each do |run| %>
                   <div class="widget_summary">
                     <div class="w_left w_55">
                        <a href="/list_executions?execution_id=<%= run[:execution_id] %>"> <%= run[:test_name] %> (<%= run[:environment]  %>)</a>
                     </div>


                    <div class="w_right w_20">
                       <span><%= run[:execution_status] %></span>
                     </div>
                   </div>
                   <% end %>
                <% end %>
            <% end %>
	 	   </div>
       </div>
   </div>
</div>