
<html>
	<body>
		<div class="col-md-12 col-sm-12 col-xs-12">
			<h1> DashBoard in Progress ... </h1>
			<div class="clearfix"></div>


			 <div class="col-md-3 col-sm-12 col-xs-12">           
                <div class="x_panel fixed_height_320">
		            <div class="x_title">
		              <h2>Last 4 Executionss</h2>		           
		              <div class="clearfix"></div>
		            </div>
		            <div class="x_content"> 		          		
			            <ul class="list-unstyled top_profiles scroll-view">
			              <% @executions[0..3].each do |exec| %>	
			              <li class="media event">
			                <a class="pull-left border-aero profile_thumb">
			                  <i class="fa fa-user aero"></i>
			                </a>
			                <div class="media-body">
			                  <a class="title" href="/list_executions?execution_id=<%= exec[:execution_id] %>"><%= exec[:test_name] %></a>
			              	  <p><strong><%= exec[:status] %></strong></p>
			                  <p> <small><%= exec[:tf_start] %></small></p>			 
			                </div>
			              </li>   
			              <% end %>                   			            
			            </ul>		          
			        </div>
			    </div>		      
	        </div>
					

			<div class="col-md-4 col-sm-6 col-xs-12">
				<div class="x_panel fixed_height_320">
					<div class="x_title">
						<h2>Last 5 Analyzers</h2>
						<div class="clearfix"></div>
					</div>

					<div class="x_content">                        
						<% @analysis[0..4].each do |analysis| %>					
				    	<div class="widget_summary">
				    		<div class="w_left w_25">
				                  <a href="/list_analysis?analysis_id=<%= analysis[:analysis_id] %>"> <%= analysis[:test_name] %></a>
				            </div>
				            <div class="w_center w_55">
				                <div class="progress">
			    	                  <div class="progress-bar bg-green" role="progressbar" aria-valuenow="60" aria-valuemin="0" 	aria-valuemax="100" style="width: <%= analysis[:success_rate] %>%;">
			                        	<span class="sr-only"><%= analysis[:success_rate] %>% Complete</span>
			                      	</div>
		                    	</div>
			            	</div>
				    		<div class="w_right w_20">
			                	<span><%= analysis[:success_rate] %>%</span>
			            	</div>
			            	<div class="clearfix"></div>
				    	</div>
			    		<% end %>
		    		</div>
				</div>		
			</div>			
  			
	</body>
</html>

