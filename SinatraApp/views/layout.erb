        <!-- page content -->
        <div class="right_col" role="main">
          <!-- top tiles -->
          <div class="row tile_count">
            <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-user"></i> Total Executions</span>
              <div class="count"><%= @executions_total_count %></div>
              <span class="count_bottom"><i class="green"><%= @weekly_executions_count%></i> From last Week</span>
            </div>
             <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Total Analyzed Pages</span>
              <div class="count green"><%= @pages_total_count %></div>
              <span class="count_bottom"><i class="red"><i class="fa fa-sort-asc"></i><%= @pages_failed_count%> </i> Failed </span>
            </div>
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Total Analyzed Transactions</span>
              <div class="count"><%= @trans_total_count %></div>
              <span class="count_bottom"><i class="red"><i class="fa fa-sort-asc"></i><%= @trans_failed_count%> </i> Failed </span>
            </div>
            <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Total Analyzed WorkUnits</span>
              <div class="count green"><%= @wu_total_count %></div>
              <span class="count_bottom"><i class="red"><i class="fa fa-sort-asc"></i><%= @wu_failed_count%> </i> Failed </span>
            </div>          
            <div class="col-md-3 col-sm-4 col-xs-6 tile_stats_count">
              <span class="count_top"><i class="fa fa-clock-o"></i> Total Analyzed WorkUnits Attributes</span>
              <div class="count"><%= @wuattr_total_count %></div>
              <span class="count_bottom"><i class="red"><i class="fa fa-sort-asc"></i><%= @wuattr_failed_count%> </i> Failed </span>
            </div>            
          </div>
          <!-- /top tiles -->
         
         <!-- man page -->
          <div class="row">
             <div>
                  <%= yield %>
            </div>         
          </div>     
        </div>
      </div>          
    </div>

